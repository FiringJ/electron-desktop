{
  "name": "electron-desktop",
  "private": true,
  "main": "main.ts",
  "version": "0.0.0",
  "type": "module",
  "build": {
    "appId": "com.example.app",
    "directories": {
      "output": "build"
    },
    "mac": {
      "target": [
        "dmg",
        "zip"
      ]
    },
    "win": {
      "target": [
        "nsis",
        "zip"
      ]
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "electron:serve": "electron .",
    "electron": "vite build && electron .",
    "electron:build": "vite build && electron-builder",
    "electron:build-darwin": "vite build && electron-builder --mac"
  },
  "dependencies": {
    "element-plus": "^2.2.16",
    "vue": "^3.2.37",
    "vue-router": "^4.1.5"
  },
  "devDependencies": {
    "@types/node": "^18.7.16",
    "@vitejs/plugin-vue": "^3.1.0",
    "autoprefixer": "^10.4.8",
    "electron": "^20.1.3",
    "electron-builder": "^23.6.0",
    "postcss": "^8.4.16",
    "sass": "^1.54.9",
    "tailwindcss": "^3.1.8",
    "unplugin-auto-import": "^0.11.2",
    "unplugin-vue-components": "^0.22.7",
    "vite": "^3.1.0",
    "vite-plugin-pages": "^0.26.0",
    "vite-plugin-vue-layouts": "^0.7.0"
  }
}
